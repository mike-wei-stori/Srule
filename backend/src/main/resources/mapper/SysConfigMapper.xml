<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stori.rule.mapper.SysConfigMapper">

    <select id="selectByKey" resultType="com.stori.rule.entity.SysConfig">
        SELECT * FROM sys_config WHERE config_key = #{key}
    </select>

    <delete id="deleteByKey">
        DELETE FROM sys_config WHERE config_key = #{key}
    </delete>

</mapper>

